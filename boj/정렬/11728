#include<iostream>

using namespace std;

int A[1000001];
int B[1000001];
int res[2000002];

int main() {
	long long n, m;
	int a_count=0,b_count=0;
	cin >> n >> m;

	for (int i = 0; i < n; i++) cin >> A[i];
	for (int j = 0; j < m; j++) cin >> B[j];

	while(1){
		if (a_count + b_count == n + m) break;

		if (a_count == n ) {
			res[a_count+b_count] = B[b_count];
			b_count++;
			continue;
		}

		if (b_count == m ) {
			res[a_count + b_count] = A[a_count];
			a_count++;
			continue;
		}

		if (A[a_count] < B[b_count]) {
			res[a_count + b_count] = A[a_count];
			a_count++;
		}
		else {
			res[a_count + b_count] = B[b_count];
			b_count++;
		}
	}

	for (int i = 0; i < n + m; i++) {
		cout << res[i]<< " ";
	}
}

//////////////////////////////시간초과///////////////////////////////////////
///////////////////////// 재도전/////////////////////
