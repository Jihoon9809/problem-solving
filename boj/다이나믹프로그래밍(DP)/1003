#include <iostream>

using namespace std;

int arr[41];

int main() {
	ios::sync_with_stdio(0);
	cin.tie(0);

	int n,num;
	cin >> n;

	arr[0] = 1;
	arr[1] = 1;

	for (int i = 2; i <=40; i++) arr[i] = arr[i - 1] + arr[i - 2];

	while (n--) {
		cin >> num;
		cout << arr[num - 2] << " " << arr[num - 1];
	}

}


