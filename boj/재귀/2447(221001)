#include <iostream>

using namespace std;

int board[2200][2200];

void check(int row,int col, int n) {
	n = n / 3;
	for (int i = row + n; i < n*2 + row; i++) {
		for (int j = col + n; j < n*2 + col; j++) {
			board[i][j] = 1;
		}
	}
}

void solution(int row,int col, int n) {
	if (n < 3) return;
	check(row, col, n);
	
		for (int i = 0; i < 3; i++) {
			for (int j = 0; j < 3; j++) {
				solution(row + i * n / 3, col + j * n / 3, n / 3);
			}
		}
	
}

int main() {
	int n;
	cin >> n;

	solution(0, 0, n);
	for (int i = 0; i < n; i++) {
		for (int j = 0; j < n; j++) {
			if (board[i][j] == 0) cout << '*';
			else cout << " ";
		}
		cout << "\n";
	}
}


